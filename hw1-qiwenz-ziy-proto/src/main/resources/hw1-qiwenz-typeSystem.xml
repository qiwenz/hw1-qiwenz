<?xml version="1.0" encoding="UTF-8"?>

<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <name>hw1-qiwenz-typeSystem</name>
    <description/>
    <version>1.0</version>
    <vendor/>
  <types>
    <typeDescription>
      <name>TestElement</name>
      <description>whether a Q OR an A</description>
      <supertypeName>base</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>Token</name>
      <description>token</description>
      <supertypeName>base</supertypeName>
    </typeDescription>
  <typeDescription>
      <name>Question</name>
      <description>Q</description>
      <supertypeName>base</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>Answer</name>
      <description>To store Answers</description>
      <supertypeName>base</supertypeName>
      <features>
        <featureDescription>
          <name>isCorrect</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>NGram</name>
      <description>NGram</description>
      <supertypeName>base</supertypeName>
      <features>
        <featureDescription>
          <name>Element</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>Token</elementType>
        </featureDescription>
        <featureDescription>
          <name>ElementType</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>base</name>
      <description>base</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>confidence</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>source</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>AnswerScore</name>
      <description>the score of each answer</description>
      <supertypeName>Answer</supertypeName>
      <features>
        <featureDescription>
          <name>score</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>answer</name>
          <description/>
          <rangeTypeName>Answer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>Evaluation</name>
      <description>Top N</description>
      <supertypeName>base</supertypeName>
      <features>
        <featureDescription>
          <name>answerscore</name>
          <description/>
          <rangeTypeName>AnswerScore</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>precision</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    </types>
</typeSystemDescription>
